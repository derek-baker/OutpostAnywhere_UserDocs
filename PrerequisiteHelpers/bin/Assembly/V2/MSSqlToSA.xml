    <!-- xml -->
    <!-- xml/long varchar/long nvarchar/long binary can all be used for xml data type -->
    <!-- xml is UTF-8, long varchar is also UTF-8, long nvarchar is UTF-16/UCS-2 -->
    <!-- Using long binary can avoid encoding problems -->
    <!-- Using xml may cause xml validating problems -->
    <dtm:DataTypeMapping >
        <dtm:SourceDataType>
            <dtm:DataTypeName>xml</dtm:DataTypeName>
        </dtm:SourceDataType>
        <dtm:DestinationDataType>
            <dtm:SimpleType>
                <dtm:DataTypeName>long varchar</dtm:DataTypeName>
            </dtm:SimpleType>
        </dtm:DestinationDataType>
    </dtm:DataTypeMapping>

