	<!-- xml -->
	<!-- xml/varchar(max)/nvarchar(max)/binary(max) can all be used for xml data type -->
	<!-- xml is UTF-8, varchar is also UTF-8, nvarchar is UTF-16/UCS-2 -->
	<!-- Using binary(max) can avoid encoding problems -->
	<!-- Using xml may cause xml validating problems -->
    
	<dtm:DataTypeMapping >
		<dtm:SourceDataType>
			<dtm:DataTypeName>xml</dtm:DataTypeName>
		</dtm:SourceDataType>
		<dtm:DestinationDataType>
