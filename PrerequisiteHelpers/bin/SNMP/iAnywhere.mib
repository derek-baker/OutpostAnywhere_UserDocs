saConnected                    OBJECT-TYPE
	SYNTAX			Integer32
	MAX-ACCESS		read-write
	STATUS			current
	DESCRIPTION
	    "Returns 1 if the agent is connected to this database, and 0
             if not. Setting the value to 1 or 0 makes the agent connect
             or disconnect."
	::= { saAgent 3 }

saStarted                      OBJECT-TYPE
	SYNTAX			Integer32
	MAX-ACCESS		read-write
	STATUS			current
	DESCRIPTION
	    "Returns 1 if the database is running, and 0 if not. Setting
             the value to 1 or 0 makes the agent start or stop the
             database. (Stopping the database also causes a disconnect.)"
	::= { saAgent 4 }

saProc                         OBJECT-TYPE
	SYNTAX			DisplayString
	MAX-ACCESS		read-write
	STATUS			current
	DESCRIPTION
	    "Getting this variable returns an empty string. Setting this
             variable to a string 'foo' will attempt to execute procedure
             'foo'. Any result set or return value is discarded."
	::= { saAgent 5 }

saRestart                      OBJECT-TYPE
	SYNTAX			Integer32
	MAX-ACCESS		read-write
	STATUS			current
	DESCRIPTION
	    "Getting this variable returns 0. Setting this variable to any
             non-zero value tells the agent to effectively restart itself. All
             connections are closed, and the sasnmp.ini file is re-read."
	::= { saAgent 6 }

saInifile                      OBJECT-TYPE
	SYNTAX			DisplayString
	MAX-ACCESS		read-only
	STATUS			current
